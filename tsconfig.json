{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "strictNullChecks": true,
    "allowJs": true,
    "isolatedModules": true,
    "moduleResolution": "node",
    "types": ["astro/client"],
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "@components/*": ["./src/components/*"],
      "@layouts/*": ["./src/layouts/*"],
      "@services/*": ["./src/services/*"],
      "@pages/*": ["./src/pages/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}
